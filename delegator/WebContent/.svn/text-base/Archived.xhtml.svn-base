<!DOCTYPE composition PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
    "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<ui:composition xmlns="http://www.w3.org/1999/xhtml"
    xmlns:s="http://jboss.com/products/seam/taglib"
    xmlns:ui="http://java.sun.com/jsf/facelets"
    xmlns:f="http://java.sun.com/jsf/core"
    xmlns:h="http://java.sun.com/jsf/html"
    xmlns:rich="http://richfaces.org/rich"
    template="layout/template.xhtml">

<ui:define name="body">

    <rich:panel>
        <f:facet name="header">Archived Details</f:facet>

        <s:decorate id="tid" template="layout/display.xhtml">
            <ui:define name="label">Tid</ui:define>
            <h:outputText value="#{archivedHome.instance.tid}"/>
        </s:decorate>

        <s:decorate id="cdate" template="layout/display.xhtml">
            <ui:define name="label">Cdate</ui:define>
            <h:outputText value="#{archivedHome.instance.cdate}">
                <s:convertDateTime type="both" dateStyle="short"/>
            </h:outputText>
        </s:decorate>

        <s:decorate id="edate" template="layout/display.xhtml">
            <ui:define name="label">Edate</ui:define>
            <h:outputText value="#{archivedHome.instance.edate}">
                <s:convertDateTime type="both" dateStyle="short"/>
            </h:outputText>
        </s:decorate>

        <s:decorate id="title" template="layout/display.xhtml">
            <ui:define name="label">Title</ui:define>
            <h:outputText value="#{archivedHome.instance.title}"/>
        </s:decorate>

        <s:decorate id="description" template="layout/display.xhtml">
            <ui:define name="label">Description</ui:define>
            <h:outputText value="#{archivedHome.instance.description}"/>
        </s:decorate>

        <s:decorate id="attachment" template="layout/display.xhtml">
            <ui:define name="label">Attachment</ui:define>
            <h:outputText value="#{archivedHome.instance.attachment}"/>
        </s:decorate>

        <s:decorate id="status" template="layout/display.xhtml">
            <ui:define name="label">Status</ui:define>
            <h:outputText value="#{archivedHome.instance.status}"/>
        </s:decorate>

        <s:decorate id="parallel" template="layout/display.xhtml">
            <ui:define name="label">Parallel</ui:define>
            <h:outputText value="#{archivedHome.instance.parallel}"/>
        </s:decorate>

        <div style="clear:both"/>

    </rich:panel>

    <div class="actionButtons">

        <s:button view="/ArchivedEdit.xhtml"
                    id="edit"
                 value="Edit"/>

        <s:button view="/#{empty archivedFrom ? 'ArchivedList' : archivedFrom}.xhtml"
                    id="done"
                 value="Done"/>

    </div>

    <rich:tabPanel switchType="ajax">

    <rich:tab>
        <f:facet name="label">
            <h:panelGroup><h:graphicImage value="/img/manytoone.gif" style="vertical-align: middle; padding-right: 4px;"/>Archived by parent task id</h:panelGroup>
        </f:facet>
    <div class="association" id="archivedByParentTaskIdParent">

        <h:outputText value="There is no archivedByParentTaskId associated with this archived."
                   rendered="#{archivedHome.instance.archivedByParentTaskId == null}"/>

        <rich:dataTable var="_archived"
                   value="#{archivedHome.instance.archivedByParentTaskId}"
                rendered="#{archivedHome.instance.archivedByParentTaskId != null}"
              rowClasses="rvgRowOne,rvgRowTwo"
                      id="archivedByParentTaskIdTable">
            <h:column>
                <f:facet name="header">Tid</f:facet>
                <h:outputText value="#{_archived.tid}"/>
            </h:column>
            <h:column>
                <f:facet name="header">Archived by parent task id tid</f:facet>
                <h:outputText value="#{_archived.archivedByParentTaskId.tid}"/>
            </h:column>
            <h:column>
                <f:facet name="header">Archived by root task id tid</f:facet>
                <h:outputText value="#{_archived.archivedByRootTaskId.tid}"/>
            </h:column>
            <h:column>
                <f:facet name="header">Employee eid</f:facet>
                <h:outputText value="#{_archived.employee.eid}"/>
            </h:column>
            <h:column>
                <f:facet name="header">Cdate</f:facet>
                <h:outputText value="#{_archived.cdate}">
                    <s:convertDateTime type="both" dateStyle="short"/>
                </h:outputText>
            </h:column>
            <h:column>
                <f:facet name="header">Edate</f:facet>
                <h:outputText value="#{_archived.edate}">
                    <s:convertDateTime type="both" dateStyle="short"/>
                </h:outputText>
            </h:column>
            <h:column>
                <f:facet name="header">Title</f:facet>
                <h:outputText value="#{_archived.title}"/>
            </h:column>
            <h:column>
                <f:facet name="header">Description</f:facet>
                <h:outputText value="#{_archived.description}"/>
            </h:column>
            <h:column>
                <f:facet name="header">Attachment</f:facet>
                <h:outputText value="#{_archived.attachment}"/>
            </h:column>
            <h:column>
                <f:facet name="header">Status</f:facet>
                <h:outputText value="#{_archived.status}"/>
            </h:column>
            <h:column>
                <f:facet name="header">Parallel</f:facet>
                <h:outputText value="#{_archived.parallel}"/>
            </h:column>
            <h:column styleClass="action">
                <f:facet name="header">Action</f:facet>
                <s:link id="viewarchived"
                     value="View"
                      view="/Archived.xhtml">
                    <f:param name="archivedTid"
                           value="#{_archived.tid}"/>
                </s:link>
            </h:column>
        </rich:dataTable>

    </div>
    </rich:tab>

    <rich:tab>
        <f:facet name="label">
            <h:panelGroup><h:graphicImage value="/img/manytoone.gif" style="vertical-align: middle; padding-right: 4px;"/>Archived by root task id</h:panelGroup>
        </f:facet>
    <div class="association" id="archivedByRootTaskIdParent">

        <h:outputText value="There is no archivedByRootTaskId associated with this archived."
                   rendered="#{archivedHome.instance.archivedByRootTaskId == null}"/>

        <rich:dataTable var="_archived"
                   value="#{archivedHome.instance.archivedByRootTaskId}"
                rendered="#{archivedHome.instance.archivedByRootTaskId != null}"
              rowClasses="rvgRowOne,rvgRowTwo"
                      id="archivedByRootTaskIdTable">
            <h:column>
                <f:facet name="header">Tid</f:facet>
                <h:outputText value="#{_archived.tid}"/>
            </h:column>
            <h:column>
                <f:facet name="header">Archived by parent task id tid</f:facet>
                <h:outputText value="#{_archived.archivedByParentTaskId.tid}"/>
            </h:column>
            <h:column>
                <f:facet name="header">Archived by root task id tid</f:facet>
                <h:outputText value="#{_archived.archivedByRootTaskId.tid}"/>
            </h:column>
            <h:column>
                <f:facet name="header">Employee eid</f:facet>
                <h:outputText value="#{_archived.employee.eid}"/>
            </h:column>
            <h:column>
                <f:facet name="header">Cdate</f:facet>
