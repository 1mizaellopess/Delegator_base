                    <f:param name="archivedFrom" value="Archived"/>
                </s:link>
            </h:column>
        </rich:dataTable>

    </h:form>

    <div class="actionButtons">
        <s:button
               value="Add archived"
                view="/ArchivedEdit.xhtml">
            <f:param name="archivedTid"
                    value="#{archivedHome.instance.tid}"/>
            <f:param name="archivedFrom" value="Archived"/>
        </s:button>
    </div>
    </rich:tab>
</rich:tabPanel>
</ui:define>

</ui:composition>
